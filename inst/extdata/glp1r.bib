  keywords = {Adult Children,Aged,Cohort Studies,Cross-Sectional Studies,Echocardiography Doppler Color,Female,Heart Failure,Humans,Hypertrophy Left Ventricular,Incidence,Longitudinal Studies,Male,Middle Aged,Parents,Prevalence,Proportional Hazards Models,Risk,Ventricular Dysfunction Left},
