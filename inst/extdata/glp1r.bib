@article{Bart2012,
  title = {Ultrafiltration in Decompensated Heart Failure with Cardiorenal Syndrome},
  author = {Bart, Bradley A. and Goldsmith, Steven R. and Lee, Kerry L. and Givertz, Michael M. and O'Connor, Christopher M. and Bull, David A. and Redfield, Margaret M. and Deswal, Anita and Rouleau, Jean L. and LeWinter, Martin M. and Ofili, Elizabeth O. and Stevenson, Lynne W. and Semigran, Marc J. and Felker, G. Michael and Chen, Horng H. and Hernandez, Adrian F. and Anstrom, Kevin J. and McNulty, Steven E. and Velazquez, Eric J. and Ibarra, Jenny C. and Mascette, Alice M. and Braunwald, Eugene},
  year = {2012},
