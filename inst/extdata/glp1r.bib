@article{lewisEffectOralIron2017,
  title = {Effect of {{Oral Iron Repletion}} on {{Exercise Capacity}} in {{Patients With Heart Failure With Reduced Ejection Fraction}} and {{Iron Deficiency}}: {{The IRONOUT HF Randomized Clinical Trial}}},
  shorttitle = {Effect of {{Oral Iron Repletion}} on {{Exercise Capacity}} in {{Patients With Heart Failure With Reduced Ejection Fraction}} and {{Iron Deficiency}}},
  author = {Lewis, Gregory D. and Malhotra, Rajeev and Hernandez, Adrian F. and McNulty, Steven E. and Smith, Andrew and Felker, G. Michael and Tang, W. H. Wilson and LaRue, Shane J. and Redfield, Margaret M. and Semigran, Marc J. and Givertz, Michael M. and Van Buren, Peter and Whellan, David and Anstrom, Kevin J. and Shah, Monica R. and {Desvigne-Nickens}, Patrice and Butler, Javed and Braunwald, Eugene and {NHLBI Heart Failure Clinical Research Network}},
  year = {2017},
