  pmid = {31970540},
  keywords = {Adiposity,Body Composition,Body Fat Distribution,Body Mass Index,Genetic Heterogeneity,GWAS,Humans,Mendelian randomization,Metabolic disease,Obesity,Obesity genetics,Obesity subtypes,Phenotype,Sex Factors,Waist Circumference}
