@article{leydenHarnessingTissuespecificGenetic2022,
  title = {Harnessing Tissue-Specific Genetic Variation to Dissect Putative Causal Pathways between Body Mass Index and Cardiometabolic Phenotypes},
  author = {Leyden, Genevieve M. and Shapland, Chin Yang and Davey Smith, George and Sanderson, Eleanor and Greenwood, Michael P. and Murphy, David and Richardson, Tom G.},
  year = {2022},
