  langid = {english},
  keywords = {Gene expression,Gene regulation,Transcriptomics},
  file = {/Users/xx20081/Zotero/storage/B457TGMA/de Klein et al. - 2023 - Brain expression quantitative trait locus and netw.pdf}
}

