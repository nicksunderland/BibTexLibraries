  pmcid = {PMC7810487},
  pmid = {33201861},
  keywords = {Adolescent,Adult,Animals,Cardiology,Cardiovascular disease,Female,Genetic Loci,Genetics,Genome-Wide Association Study,Germany,Heart Defects Congenital,Humans,iPS cells,Male,Mice,Molecular genetics,Polymorphism Single Nucleotide,Risk Factors},
