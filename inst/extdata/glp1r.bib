@article{dhindsaTranscriptomeBasedDrugDiscovery2021,
  title = {A {{Transcriptome-Based Drug Discovery Paradigm}} for {{Neurodevelopmental Disorders}}},
  author = {Dhindsa, Ryan S. and Zoghbi, Anthony W. and Krizay, Daniel K. and Vasavda, Chirag and Goldstein, David B.},
