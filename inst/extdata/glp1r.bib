@article{mottetPharmacogenomicsHeartFailure2016,
  title = {Pharmacogenomics of Heart Failure: A Systematic Review},
  shorttitle = {Pharmacogenomics of Heart Failure},
  author = {Mottet, Fannie and Vardeny, Orly and {de Denus}, Simon},
  year = {2016},
