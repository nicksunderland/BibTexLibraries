  langid = {english},
  keywords = {Heart failure,Outcomes research,Prognostic markers},
  file = {/Users/xx20081/Zotero/storage/7QVQJ4A9/Stryeck et al. - 2019 - Serum Concentrations of Citrate, Tyrosine, 2- and 3- Hydroxybutyrate are Associated with Increased 3.pdf}
}

