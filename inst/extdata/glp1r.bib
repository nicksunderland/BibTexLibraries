  keywords = {Administration Oral,Aged,Double-Blind Method,Exercise Tolerance,Female,Ferritins,Health Status,Heart Failure,Humans,Iron Compounds,Iron Deficiencies,Male,Middle Aged,Natriuretic Peptide Brain,Oxygen Consumption,Peptide Fragments,Quality of Life,Stroke Volume,Time Factors,Transferrin,Treatment Outcome,Walk Test},
  file = {/Users/xx20081/Zotero/storage/UPD8UTVF/Lewis et al. - 2017 - Effect of Oral Iron Repletion on Exercise Capacity.pdf}
}

