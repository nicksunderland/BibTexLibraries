@article{wingoSexDifferencesBrain2023,
  title = {Sex Differences in Brain Protein Expression and Disease},
  author = {Wingo, Aliza P. and Liu, Yue and Gerasimov, Ekaterina S. and Vattathil, Selina M. and Liu, Jiaqi and Cutler, David J. and Epstein, Michael P. and Blokland, Gabri{\"e}lla A. M. and Thambisetty, Madhav and Troncoso, Juan C. and Duong, Duc M. and Bennett, David A. and Levey, Allan I. and Seyfried, Nicholas T. and Wingo, Thomas S.},
  year = {2023},
