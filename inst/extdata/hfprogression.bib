  keywords = {4C-sequencing,{Adaptor Proteins, Signal Transducing},Animals,Apoptosis Regulatory Proteins,{Cardiomyopathy, Dilated},Chromosomes,Dilated cardiomyopathy,Genetic Predisposition to Disease,Genetic risk score,Genome-Wide Association Study,GWAS,Heart failure,{Heart Failure, Systolic},Humans,Imputation,{Polymorphism, Single Nucleotide}},
