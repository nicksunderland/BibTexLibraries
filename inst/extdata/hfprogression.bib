  langid = {english},
  keywords = {Genome-wide association studies,Parkinson's disease},
  file = {/Users/xx20081/Zotero/storage/LVW9RZAX/Liu et al. - 2021 - Genome-wide survival study identifies a novel syna.pdf}
}

