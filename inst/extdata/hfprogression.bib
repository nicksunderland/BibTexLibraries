  keywords = {Computational models,Epidemiology,Genetics research,Machine learning,Risk factors},
  file = {/Users/xx20081/Zotero/storage/WBHRXICU/Mahmoud et al. - 2022 - A robust method for collider bias correction in co.pdf}
}

